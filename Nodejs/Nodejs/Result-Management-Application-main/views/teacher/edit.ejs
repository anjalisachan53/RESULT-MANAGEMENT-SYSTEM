<a href="/teacher/viewall" class="btn btn-info" style="margin-top:5px;">Back</a>
<h4>Editing Form for Student :<%= user.Name %> </h4>
<form method="POST" class="addStu" autocomplete="off">

  <div class="mb-3">
    <label for="exampleInputEmail1" class="form-label">Roll No. : </label>
    <input type="number" name="Rollnum" value="<%= user.Rollnum%>" required class="form-control" id="exampleInputEmail1" >
  </div>  

    <div class="mb-3">
      <label for="exampleInputEmail1" class="form-label">Name : </label>
      <input type="text" name="Name" required class="form-control" value="<%= user.Name%>" id="exampleInputEmail1">
    </div>
     
      <div class="mb-3">
        <label for="exampleInputEmail1" class="form-label">Date of Birth : </label>
        <input type="date" name="DOB" value="<%= user.DOB%>" required class="form-control" id="exampleInputEmail1">
      </div>
      <div class="mb-3">
        <label for="exampleInputEmail1" class="form-label">Score : </label>
        <input type="number" name="Score" value="<%= user.Score%>" required class="form-control" id="exampleInputEmail1">
      </div>
    <button type="submit" class="btn btn-success">Update</button>
</form>
